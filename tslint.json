{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "curly": false,
    "indent": [
      true,
      "spaces"
    ],
    "interface-name": false,
    "max-line-length": [
      true,
      200
    ],
    "no-angle-bracket-type-assertion": false,
    "no-bitwise": false,
    "no-console": [
      true,
      "time",
      "timeEnd"
    ],
    "no-empty": false,
    "no-trailing-whitespace": true,
    "object-literal-shorthand": false,
    "object-literal-sort-keys": false,
    "one-line": [
      true,
      "check-whitespace"
    ],
    "one-variable-per-declaration": false,
    "prefer-const": true,
    "quotemark": [
      true,
      "single"
    ]
  },
  "rulesDirectory": []
}
